{
    "type": "sqlite",
    "database": "./src/database/database.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
       "src/model/**/*.ts"
    ],
    "migrations": [
       "src/database/migration/**/*.ts"
    ],
    "cli": {
       "entitiesDir": "src/entity",
       "migrationsDir": "src/database/migration"
    }
}